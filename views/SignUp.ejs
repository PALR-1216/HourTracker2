  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="https://cdn.tailwindcss.com"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/1.6.5/datepicker.min.js"></script>

</head>

<body>
  <meta name="viewport" content="width=device-width, user-scalable=no">


  <div class="flex min-h-full flex-col justify-center px-6 py-12 lg:px-8">
    <div class="sm:mx-auto sm:w-full sm:max-w-sm">
      <lottie-player src="https://assets3.lottiefiles.com/packages/lf20_VbfZ29h8vM.json"  background="transparent"  speed="1"   loop  autoplay></lottie-player>
      <h2 class="mt-10 text-center text-2xl font-bold leading-9 tracking-tight text-gray-900">Become a Member</h2>
    </div>

    <div class="mt-10 sm:mx-auto sm:w-full sm:max-w-sm">
      <form class="space-y-6" action="/SignUpAuth" method="POST" name="SignUp" enctype="x-www-form-urlencoded">
        <div>
          <label for="userName" class="block text-sm font-medium leading-6 text-gray-900">User Name?</label>
          <div class="mt-2">
            <input id="userName" name="userName" type="text" required
              class="block w-full  rounded-md border border-indigo-500 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6 ">
          </div>
        </div>

        <div>
          <label for="Email" class="block text-sm font-medium leading-6 text-gray-900">Email?</label>
          <div class="mt-2">
            <input id="Email" name="Email" type="email" required
              class="block w-full  rounded-md border border-indigo-500 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6 ">
          </div>
        </div>

        <div>
          <label for="Wage" class="block text-sm font-medium leading-6 text-gray-900">Your Wage?</label>
          <div class="mt-2">
            <input id="Wage" name="Wage" type="number" step="0.1" pattern="\d*" placeholder="Amount you earn $" inputmode="decimal"
              required min="0" step="any"
              class="block w-full  rounded-md border border-indigo-500 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6 ">
          </div>
        </div>

        <div>
          <label for="Deduction" class="block text-sm font-medium leading-6 text-gray-900">Your Deduction porcentage?
          </label>
          <div class="mt-2">
            <input id="Deduction" pattern="\d*" name="Deduction" placeholder="Enter Porcent %" type="number"
              inputmode="decimal" required min="0" step=".1"
              class="block w-full  rounded-md border border-indigo-500 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6 ">
          </div>
        </div>


        <div>
          <label for="DatePicker" id="DatePicker" class="block text-sm font-medium leading-6 text-gray-900">Select The date of the period Starts</label>
          <div class="mt-2">
            <input id="Date" name="DatePicker" style="width: 380px; height: 40px; font-size: 15px;" type="date" min="" required
              class="block w-full  rounded-md border border-indigo-500 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6 ">
              </div>
              
          </div>

          <div>
            <label for="DatePicker" id="DatePicker" class="block text-sm font-medium leading-6 text-gray-900">Select The date of the period End</label>
            <div class="mt-2">
              <input id="Date" name="DatePickerEnd" style="width: 380px; height: 40px; font-size: 15px;" type="date" required
                class="block w-full  rounded-md border border-indigo-500 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6 ">
                </div>
                
            </div>

            <div>
              <label for="payOut" id="payOut" class="block text-sm font-medium leading-6 text-gray-900">Select your next PayOut</label>
              <div class="mt-2">
                <input id="Date" name="payOut" style="width: 380px; height: 40px; font-size: 15px;" type="date" required
                  class="block w-full  rounded-md border border-indigo-500 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6 ">
                  </div>
                  
              </div>
            

        <main class="">
          <!-- tabs -->
          <label >Whats your overtime type?</label>
          <br/>
          
          
          <div
            class="grid w-[24rem] grid-cols-2 space-x-2 rounded-xl bg-gray-200 p-2"
            x-data="app"
          >
            <div>
              <input type="radio" name="OvertimeType" id="Half" value="Half" class="peer hidden"  checked />
              <label
                for="Half"
                class="block cursor-pointer select-none rounded-xl p-2 text-center peer-checked:bg-blue-500 peer-checked:font-bold peer-checked:text-white"
                >Time and Half</label
              >
            </div>
        
            <div>
              <input type="radio" name="OvertimeType" id="Double" value="Double" class="peer hidden" />
              <label
                for="Double"
                class="block cursor-pointer select-none rounded-xl p-2 text-center peer-checked:bg-blue-500 peer-checked:font-bold peer-checked:text-white"
                > Double Time</label
              >
            </div>
          </div>
        </main>





        <main class="">
          <!-- tabs -->
          <label >Payment Shedule</label>
          <br/>
          
          
          <div
            class="grid w-[24rem] grid-cols-3 space-x-2 rounded-xl bg-gray-200 p-2"
            x-data="app"
          >
            <div>
              <input type="radio" name="PaymentRate" id="Weekly" class="peer hidden" checked value="Weekly" />
              <label
                for="Weekly"
                class="block cursor-pointer select-none rounded-xl p-2 text-center peer-checked:bg-blue-500 peer-checked:font-bold peer-checked:text-white"
                >Weekly</label
              >
            </div>
        
            <div>
              <input type="radio" name="PaymentRate" id="Biweekly" class="peer hidden" value="Biweekly" />
              <label
                for="Biweekly"
                class="block cursor-pointer select-none rounded-xl p-2 text-center peer-checked:bg-blue-500 peer-checked:font-bold peer-checked:text-white"
                >Biweekly</label
              >
            </div>
        
            <div>
              <!-- still needs work -->
              <input type="radio" name="EndPeriodDate" id="Monthly" value="Monthly" class="peer hidden" />
              <label
                for="Monthly"
                class="block cursor-pointer select-none rounded-xl p-2 text-center peer-checked:bg-blue-500 peer-checked:font-bold peer-checked:text-white"
                >Monthly</label
              >
            </div>

          </div>
        </main>

        <div>

          <!-- <label for="profilePic"
            class="flex items-center p-3.5 gap-3 rounded-3xl border border-gray-300 border-dashed bg-gray-50 cursor-pointer">
            <img class="h-16 w-auto" src="https://i.ibb.co/FH3FN7F/file.png" alt="">.
            <div class="space-y-2">
              <h4 class="text-base font-semibold text-gray-700">Upload your Profile image</h4> -->
              <!-- <span class="text-sm text-gray-500">Max 2 MO</span> -->
            <!-- </div>
            <input type="file" id="profilePic" name="profilePic" accept=".png, .jpg, .jpeg" hidden />
          </label> -->

          <div class="flex items-center justify-between">
            <label for="Password" class="block text-sm font-medium leading-6 text-gray-900">Password</label>

          </div>
          <div class="mt-2">
            <input id="Password" name="Password" type="password" required
              class="block w-full  rounded-md border border-indigo-500 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6 ">
          </div>
        </div>

        <div>
          <button type="submit"
            class="flex w-full justify-center rounded-md bg-indigo-600 px-3 py-1.5 text-sm font-semibold leading-6 text-white shadow-sm hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600">Sign
            in</button>
        </div>
      </form>
      
      

      <p class="mt-10 text-center text-sm text-gray-500">
        Already a Member?
        <a href="/Login" class="font-semibold leading-6 text-indigo-600 hover:text-indigo-500">Login</a>
      </p>
    </div>
  </div>

 

  <style>

@media only screen and (max-width: 600px) {
  #Logo {

    width: 250px;
    height: 250px;

  }
}





@media only screen and (min-width: 600px) {
  #Logo {

    width: 320px;
    height: 320px;

  }
}
    
  </style>


</body>

</html>